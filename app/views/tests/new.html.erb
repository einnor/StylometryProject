<%= form_for([@student, @test], :html => {:multipart => true}) do |f| %>
      
        <% if @test.errors.any? %>
          <div id="error_explanation">
            <h2>
              <%= pluralize(@test.errors.count, "error") %> prohibited this student from being saved:
            </h2>
            <ul>
              <%= test.errors.full_messages.each do |message| %>
                <li><%= message %></li>
              <% end %>
            </ul>
          </div>  
        <% end %>
        
        <div class="field"><%= f.label :evaluate_to_be_evaluated %> <br />
          <%= f.file_field :evaluate %>
        </div>
        
        <div class="actions">
          <%= f.submit "Submit", :class=>"btn btn-primary" %>
        </div>
      <% end %>